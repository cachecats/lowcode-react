{
  "settings": [
    {
      "id": "dataSource",
      "label": "数据源",
      "type": "json",
      "defaultValue": "[]"
    },
    {
      "id": "columns",
      "label": "表格列",
      "type": "array",
      "defaultValue": [
        {
          "title": "姓名",
          "dataIndex": "name",
          "width": 100,
          "ellipsis": true,
          "copyable": false,
          "hideInSearch": false,
          "hideInTable": false
        },
        {
          "title": "年龄",
          "dataIndex": "age",
          "width": 100,
          "ellipsis": true,
          "copyable": false,
          "hideInSearch": false,
          "hideInTable": false
        }
      ],
      "options": [
        {
          "id": "title",
          "label": "标题",
          "valueType": "text",
          "required": true
        },
        {
          "id": "dataIndex",
          "label": "数据字段",
          "valueType": "text",
          "required": true
        },
        {
          "id": "width",
          "label": "列宽",
          "valueType": "digit"
        },
        {
          "id": "ellipsis",
          "label": "ellipsis",
          "valueType": "radio",
          "options": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        },
        {
          "id": "copyable",
          "label": "可复制",
          "valueType": "radio",
          "options": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        },
        {
          "id": "hideInSearch",
          "label": "搜索中隐藏",
          "valueType": "radio",
          "options": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        },
        {
          "id": "hideInTable",
          "label": "表格中隐藏",
          "valueType": "radio",
          "options": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        }
      ]
    },
    {
      "id": "search",
      "label": "搜索栏",
      "type": "group",
      "children": [
        {
          "parentId": "search",
          "id": "isSearch",
          "label": "启用搜索",
          "type": "switch",
          "defaultValue": true
        },
        {
          "parentId": "search",
          "id": "filterType",
          "label": "筛选类型",
          "type": "radio",
          "defaultValue": "query",
          "options": [
            {
              "label": "query",
              "value": "query"
            },
            {
              "label": "light",
              "value": "light"
            }
          ]
        },
        {
          "parentId": "search",
          "id": "defaultCollapsed",
          "label": "默认收起",
          "type": "switch",
          "defaultValue": true
        }
      ]
    },
    {
      "id": "other",
      "label": "其他设置",
      "type": "group",
      "children": [
        {
          "id": "scrollX",
          "label": "scrollX",
          "type": "inputNumber",
          "addonAfter": "px",
          "controls": false,
          "tips": "表格宽度超过多少时滚动"
        }
      ]
    }
  ]
}
